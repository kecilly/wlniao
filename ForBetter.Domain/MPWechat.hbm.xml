<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="Shijia.Domain">
    <class name="Shijia.Domain.MPWechat" table="[MPWechat]">
      <cache usage="read-write"/>
      <id name="Id" type="int"  unsaved-value="0">
        <column name="Id" index="PK_Account"/>
        <generator class="identity" />
      </id>
      <property name="AccountId" column="AccountId" type="int"  />
      <property name="WeixinName" column="WeixinName" type="string"  />
      <property name="Token" column="Token" type="string"  />
      <property name="FristAccount" column="FristAccount" type="string"  />
      <property name="ManagerOpenid" column="ManagerOpenid" type="string"  />
      <property name="VerifyContent" column="VerifyContent" type="string"  />
      <property name="WelcomeMsg" column="WelcomeMsg" type="string"  />
      <property name="WelcomeMPType" column="WelcomeMPType" type="int"  />
      <property name="DefaultMsg" column="DefaultMsg" type="string"  />
      <property name="DefaultMPType" column="DefaultMPType" type="int"  />
      <property name="MPType" column="MPType" type="int"  />
      <property name="AppId" column="AppId" type="string"  />
      <property name="KeyWordsList" column="KeyWordsList" type="string"  />
      <property name="AppSecret" column="AppSecret" type="string"  />
    </class>
</hibernate-mapping>
